{
	"name": "nri_scheduler",
	"version": "0.1.0",
	"scripts": {
		"start": "node start.js",
		"dev": "export $(cat .env | grep -v '^#' | xargs) && vite",
		"force": "vite --force",
		"build": "tsc && vite build",
		"build_env": "tsc && export $(cat .env | grep -v '^#' | xargs) && vite build",
		"preview": "npm run build && export $(cat .env | grep -v '^#' | xargs) && vite preview",
		"tsc": "tsc",
		"prebuild": "rm -rf static || npx rimraf static",
		"lint": "eslint ./client"
	},
	"dependencies": {
		"@chakra-ui/react": "^3.16.1",
		"@choc-ui/chakra-autocomplete": "^6.1.0",
		"@nanostores/react": "^1.0.0",
		"@schedule-x/calendar": "^2.28.0",
		"@schedule-x/react": "^2.28.0",
		"@schedule-x/shared": "^2.28.0",
		"@schedule-x/theme-default": "^2.28.0",
		"dayjs": "^1.11.13",
		"nanostores": "^1.0.1",
		"procetar": "^0.3.2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-hook-form": "^7.56.1",
		"react-icons": "^5.5.0",
		"react-router": "^7.5.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.25.1",
		"@types/node": "^22.15.2",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.3",
		"@vitejs/plugin-react": "^4.4.1",
		"dotenv": "^16.5.0",
		"eslint": "^9.25.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-node-import": "^1.0.4",
		"eslint-plugin-prettier": "^5.2.6",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"globals": "^16.0.0",
		"prettier": "^3.5.3",
		"prettier-eslint": "^16.4.1",
		"terser": "^5.39.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.31.0",
		"vite": "^6.3.3"
	}
}
