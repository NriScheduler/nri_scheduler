{
	"name": "nri_scheduler",
	"version": "0.1.0",
	"scripts": {
		"start": "node start.js",
		"dev": "export $(cat .env | grep -v '^#' | xargs) && vite",
		"force": "vite --force",
		"build": "tsc && vite build",
		"preview": "npm run build && export $(cat .env | grep -v '^#' | xargs) && vite preview",
		"tsc": "tsc",
		"prebuild": "rm -rf static || npx rimraf static",
		"lint": "eslint ./client"
	},
	"dependencies": {
		"@chakra-ui/react": "^3.13.0",
		"@nanostores/preact": "^0.5.2",
		"@schedule-x/calendar": "^2.22.0",
		"@schedule-x/preact": "^2.17.0",
		"@schedule-x/shared": "^2.22.0",
		"@schedule-x/theme-default": "^2.22.0",
		"dayjs": "^1.11.13",
		"nanostores": "^0.11.4",
		"preact": "^10.26.4",
		"preact-async-route": "^2.2.1",
		"preact-router": "^4.1.2",
		"procetar": "^0.3.0",
		"react-hook-form": "^7.54.2",
		"react-hot-toast": "^2.5.2",
		"react-icons": "^5.5.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.22.0",
		"@preact/preset-vite": "^2.10.1",
		"@types/node": "^22.13.10",
		"dotenv": "^16.4.7",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-node-import": "^1.0.4",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"globals": "^16.0.0",
		"prettier": "^3.5.3",
		"prettier-eslint": "^16.3.0",
		"terser": "^5.39.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.1",
		"vite": "^6.2.2"
	}
}
