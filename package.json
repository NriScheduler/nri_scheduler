{
	"name": "nri_scheduler",
	"version": "0.1.0",
	"scripts": {
		"start": "vite",
		"dev": "export $(cat .env | grep -v '^#' | xargs) && vite",
		"force": "vite --force",
		"build": "tsc && vite build",
		"preview": "npm run build && export $(cat .env | grep -v '^#' | xargs) && vite preview",
		"tsc": "tsc",
		"prebuild": "rm -rf static || npx rimraf static"
	},
	"dependencies": {
		"@nanostores/preact": "^0.5.2",
		"@nanostores/query": "^0.3.4",
		"@schedule-x/calendar": "^2.19.0",
		"@schedule-x/preact": "^2.14.0",
		"@schedule-x/theme-default": "^2.19.0",
		"dayjs": "^1.11.13",
		"nanostores": "^0.11.4",
		"preact": "^10.26.0",
		"preact-async-route": "^2.2.1",
		"preact-router": "^4.1.2",
		"purecss": "^3.0.0",
		"react-hot-toast": "^2.5.2"
	},
	"devDependencies": {
		"@preact/preset-vite": "^2.10.1",
		"@types/node": "^22.13.4",
		"terser": "^5.39.0",
		"typescript": "^5.7.3",
		"vite": "^6.1.0"
	}
}
