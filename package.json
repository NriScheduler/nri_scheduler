{
	"name": "nri_scheduler",
	"version": "0.1.0",
	"scripts": {
		"start": "node start.js",
		"dev": "export $(cat .env | grep -v '^#' | xargs) && vite",
		"force": "vite --force",
		"build": "tsc && vite build",
		"build_env": "tsc && export $(cat .env | grep -v '^#' | xargs) && vite build",
		"preview": "npm run build && export $(cat .env | grep -v '^#' | xargs) && vite preview",
		"tsc": "tsc",
		"prebuild": "rm -rf static || npx rimraf static",
		"lint": "eslint ./client"
	},
	"dependencies": {
		"@chakra-ui/react": "^3.21.0",
		"@choc-ui/chakra-autocomplete": "^6.1.3",
		"@nanostores/preact": "^1.0.0",
		"@schedule-x/calendar": "^2.34.0",
		"@schedule-x/preact": "^2.29.0",
		"@schedule-x/shared": "^2.34.0",
		"@schedule-x/theme-default": "^2.34.0",
		"dayjs": "^1.11.18",
		"nanostores": "^1.0.1",
		"preact": "^10.27.1",
		"preact-router": "^4.1.2",
		"procetar": "^0.3.2",
		"react-hook-form": "^7.58.1",
		"react-icons": "^5.5.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@preact/preset-vite": "^2.10.2",
		"@types/node": "^22.18.1",
		"dotenv": "^17.2.2",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-node-import": "^1.0.5",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"rollup": "^4.46.2",
		"terser": "^5.40.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0",
		"vite": "^7.0.6"
	}
}
